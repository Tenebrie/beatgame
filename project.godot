; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="BeatGame"
run/main_scene="res://scenes/TrainingRoom.tscn"
config/features=PackedStringArray("4.2", "C#", "Forward Plus")
run/max_fps=165
boot_splash/bg_color=Color(0.478431, 0.478431, 0.478431, 1)
boot_splash/show_image=false
boot_splash/image="res://assets/logo.png"
boot_splash/fullsize=false
boot_splash/use_filter=false
config/icon="res://icon.svg"

[autoload]

SignalBus="*res://scripts/SignalBus.cs"
Preferences="*res://scripts/preferences/Preferences.cs"
AssetManager="*res://scripts/resources/AssetManager.cs"
LoadingManager="*res://scripts/resources/LoadingManager.cs"
