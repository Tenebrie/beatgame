CombatUI="*res://ui/screens/combat/CombatUI.tscn"
FightEditorUI="*res://ui/screens/fightEditor/FightEditorUI.tscn"
DpsMeterUI="*res://ui/screens/dpsMeter/DpsMeterUI.tscn"
SkillForestUI="*res://ui/screens/skillForest/SkillForestUI.tscn"
PauseUI="*res://ui/screens/pause/PauseUI.tscn"
TransitionUI="*res://ui/screens/transition/TransitionUI.tscn"
RespawnUI="*res://ui/screens/respawn/RespawnUI.tscn"
InteractableObjectPopup="*res://objects/Interactables/InteractableObjectPopup.tscn"

[debug]

file_logging/enable_file_logging=true

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080

[dotnet]

project/assembly_name="BeatGame"

